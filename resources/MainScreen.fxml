<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="Archo" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.allControllers.MainController">
   <children>
      <BorderPane fx:id="mainBorderPane" prefHeight="600.0" prefWidth="1050.0">
         <left>
            <VBox prefHeight="400.0" prefWidth="121.0" BorderPane.alignment="CENTER">
               <children>
                  <Button fx:id="logOutBt" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#on_logout" prefHeight="31.0" prefWidth="128.0" text="log out">
                     <VBox.margin>
                        <Insets top="10.0" />
                     </VBox.margin>
                  </Button>
                  <Button fx:id="editInfoBut" mnemonicParsing="false" onAction="#edit_info_screen" prefHeight="31.0" prefWidth="128.0" text="edit info">
                     <VBox.margin>
                        <Insets top="10.0" />
                     </VBox.margin>
                  </Button>
                  <Button fx:id="bookSearch" layoutX="10.0" layoutY="61.0" mnemonicParsing="false" onAction="#Buy_screen" prefHeight="31.0" prefWidth="128.0" text="book search">
                     <VBox.margin>
                        <Insets top="10.0" />
                     </VBox.margin>
                  </Button>
                  <Button fx:id="porBt" mnemonicParsing="false" onAction="#pro_screen" prefHeight="31.0" prefWidth="128.0" text="pormotion">
                     <VBox.margin>
                        <Insets top="10.0" />
                     </VBox.margin>
                  </Button>
                  <Button fx:id="placeBt" mnemonicParsing="false" onAction="#place_book_orders" prefHeight="31.0" prefWidth="128.0" text="place">
                     <VBox.margin>
                        <Insets top="10.0" />
                     </VBox.margin>
                  </Button>
                  <Button fx:id="modify" mnemonicParsing="false" onAction="#modifyBooks" prefHeight="31.0" prefWidth="128.0" text="Modify Books">
                     <VBox.margin>
                        <Insets top="10.0" />
                     </VBox.margin>
                  </Button>
                  <Button fx:id="confirmBt" mnemonicParsing="false" onAction="#confirm_book_orders" prefHeight="31.0" prefWidth="128.0" text="confirm">
                     <VBox.margin>
                        <Insets top="10.0" />
                     </VBox.margin>
                  </Button>
                  <Button fx:id="reportBt" mnemonicParsing="false" onAction="#makeRebort" prefHeight="31.0" prefWidth="128.0" text="report">
                     <VBox.margin>
                        <Insets top="10.0" />
                     </VBox.margin>
                  </Button>
               </children>
               <BorderPane.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </BorderPane.margin>
            </VBox>
         </left>
         <center>
            <Pane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
               <children>
                  <Label fx:id="label1" contentDisplay="CENTER" layoutX="289.0" layoutY="247.0" text="WELCOME TO OUR" textAlignment="CENTER">
                     <font>
                        <Font size="30.0" />
                     </font>
                  </Label>
                  <Label fx:id="label2" layoutX="330.0" layoutY="300.0" text="BOOKSTORE!">
                     <font>
                        <Font size="30.0" />
                     </font>
                  </Label>
               </children>
            </Pane>
         </center>
      </BorderPane>
   </children>
</AnchorPane>
