<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane fx:id="Archo" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.allControllers.MainController">
   <children>
      <BorderPane fx:id="mainBorderPane" prefHeight="400.0" prefWidth="650.0">
         <left>
            <VBox prefHeight="400.0" prefWidth="121.0" BorderPane.alignment="CENTER">
               <children>
                  <Button fx:id="logOutBt" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#on_logout" prefHeight="31.0" prefWidth="128.0" text="log out">
                     <VBox.margin>
                        <Insets top="10.0" />
                     </VBox.margin>
                  </Button>
                  <Button fx:id="editInfoBut" mnemonicParsing="false" onAction="#edit_info_screen" prefHeight="31.0" prefWidth="128.0" text="edit info">
                     <VBox.margin>
                        <Insets top="10.0" />
                     </VBox.margin>
                  </Button>
                  <Button fx:id="bookSearch" layoutX="10.0" layoutY="61.0" mnemonicParsing="false" onAction="#Buy_screen" prefHeight="31.0" prefWidth="128.0" text="book search">
                     <VBox.margin>
                        <Insets top="10.0" />
                     </VBox.margin>
                  </Button>
                  <Button fx:id="porBt" mnemonicParsing="false" onAction="#pro_screen" prefHeight="31.0" prefWidth="128.0" text="pormotion">
                     <VBox.margin>
                        <Insets top="10.0" />
                     </VBox.margin>
                  </Button>
                  <Button fx:id="placeBt" mnemonicParsing="false" onAction="#place_book_orders" prefHeight="31.0" prefWidth="128.0" text="place">
                     <VBox.margin>
                        <Insets top="10.0" />
                     </VBox.margin>
                  </Button>
                  <Button fx:id="modify" mnemonicParsing="false" onAction="#modifyBooks" prefHeight="31.0" prefWidth="128.0" text="Modify Books">
                     <VBox.margin>
                        <Insets top="10.0" />
                     </VBox.margin>
                  </Button>
                  <Button fx:id="confirmBt1" mnemonicParsing="false" onAction="#confirm_book_orders" prefHeight="31.0" prefWidth="128.0" text="confirm">
                     <VBox.margin>
                        <Insets top="10.0" />
                     </VBox.margin>
                  </Button>
                  <Button fx:id="reportBt" mnemonicParsing="false" onAction="#makeRebort" prefHeight="31.0" prefWidth="128.0" text="report">
                     <VBox.margin>
                        <Insets top="10.0" />
                     </VBox.margin>
                  </Button>
               </children>
               <BorderPane.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </BorderPane.margin>
            </VBox>
         </left>
      </BorderPane>
   </children>
</AnchorPane>
